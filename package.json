{
  "name": "alice",
  "version": "0.0.1",
  "description": "Node.js web proxy",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .js -c .eslintrc.json",
    "start": "node index.js",
    "dev": "NODE_ENV=development nodemon index.js",
    "pkg": "node-deb -- ./"
  },
  "engines": {
    "node": ">=8"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NInfolab/alice.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NInfolab/alice/issues"
  },
  "homepage": "https://github.com/NInfolab/alice#readme",
  "dependencies": {
    "charset": "^1.0.0",
    "connect": "^3.4.1",
    "http-proxy": "^1.13.1",
    "iconv-lite": "^0.4.13",
    "maxmind": "^2.11.2",
    "moment": "^2.23.0",
    "multipipe": "^3.0.0",
    "pino": "^5.11.1",
    "pino-http": "^4.1.0",
    "pino-pretty": "^2.5.0",
    "replacestream": "^4.0.0"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "node-deb": "^0.10.7",
    "nodemon": "^1.18.9"
  },
  "node_deb": {
    "init": "none",
    "dependencies": "nodejs",
    "templates": {
      "postinst": "./setup.sh"
    },
    "entrypoints": {
      "daemon": "/bin/true"
    }
  },
  "nodemonConfig": {
    "ignore": [
      "metrics/*",
      "docs/*"
    ]
  }
}
